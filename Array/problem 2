/*For two strings s and t, we say "t divides s" if and only if s = t + t + t + ... + t + t (i.e., t is concatenated
 with itself one or more times).
Given two strings str1 and str2, return the largest string x such that x divides both str1 and str2.*/


#include <iostream>
#include <string>
#include <numeric>
using namespace std;

string gcdOfStrings(string str1, string str2) {
    // If no valid common divisor string exists
    if (str1 + str2 != str2 + str1)
        return "";

    // gcd of lengths
    int g = gcd(str1.size(), str2.size());

    // return prefix of length g
    return str1.substr(0, g);
}

int main() {
    string str1 = "ABCABC";
    string str2 = "ABC";
    cout << gcdOfStrings(str1, str2) << endl; // Output: "ABC"
    return 0;
}
